
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:model="sap.ui.model">
   <Table id="idEquationGroupTable" inset="true" items="{/Equations}" >
		<headerToolbar>
			<Toolbar>
			 	<Title text="{i18n>Equations}" level="H1"/>
			 </Toolbar>
		</headerToolbar>
		<columns>
			<Column>
				<Text text="Effective From" />
			</Column>
			<Column>
				<Text text="Effective To" />
			</Column>
			<Column>
				<Text text="Measurement" />
			</Column>
			<Column>
				<Text text="Equation" />
			</Column>
		</columns>                                                    
		<items>
			<ColumnListItem >
				<cells>
					<ObjectIdentifier text="{Effective From}"  />
					<Text text="{Effective To}"/>
					<Text text="{Measurement}"/>
					<Text text="{Equation}"/>
				</cells>
			</ColumnListItem>
		</items>
    </Table>
</core:FragmentDefinition>